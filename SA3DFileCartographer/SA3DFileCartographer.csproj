<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<RootNamespace>SA3DFileCarthographer</RootNamespace>
		<AssemblyName>SA3DFileCarthographer</AssemblyName>
		<Platforms>x64</Platforms>
		<DebugType>embedded</DebugType>
		<Authors>Justin113D</Authors>
		<Company>X-Hax</Company>
		<ApplicationIcon>logo.ico</ApplicationIcon>
		<Version>1.0.1</Version>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="cmdhelp.txt" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SA3D.Common" Version="1.4.7" />
		<PackageReference Include="SA3D.Modeling" Version="1.1.5" />
		<PackageReference Include="SA3D.SA2Event" Version="1.0.3" />
		<PackageReference Include="SixLabors.ImageSharp" Version="3.1.5" />
		<PackageReference Include="SixLabors.ImageSharp.Drawing" Version="2.1.4" />
	</ItemGroup>

</Project>
